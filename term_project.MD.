import requests
url = 'https://comic.naver.com/webtoon/weekday.nhn'
r = requests.get(url)
r.encoding='utf8'
html = r.text
begin=html.find('<h4 class="mon"><span>월요 웹툰')
end=html.find('(재)">킬러분식 (재)</a>')

html=html[begin:end]
html=html.replace('<p',' ')
html=html.replace('</p>',' ')

from bs4 import BeautifulSoup
soup = BeautifulSoup(html, 'html.parser')
for title in soup.select('div[class=title]'):
    prin(title)
print(soup)
