import requests
url = 'https://www.nytimes.com/section/science'
r = requests.get(url)
r.endcoding='utf8'
data = str(r.text)
begin =data.find('How to Land on Mars')
end=data.rfind('Monsters That Live On You')

data=data[begin:end]
data=data.replace('<p',' ')
data=data.replace('</p>',' ')

print(data)

